{
    "manifest_version": 3,
    "name": "Rishu's Extension",
    "version": "0.1.0",
    "description": "Tutorial extension",
    "icons": {
        "128": "./images/icon128.png",
        "48": "./images/icon48.png",
        "16": "./images/icon16.png"
    }, 
    "action": {
        "default_popup": "popup.html",
        "default_icon": "./images/icon16.png",
        "default_title": "Rishu's Extension" 
    }, 
    "permissions": [
        "tabs"
    ] 
}